<?xml version="1.0"?>
<block>
  <name>DAB: MSC decoder</name>
  <key>dab_msc_decode</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.msc_decode(dab.parameters.dab_parameters(mode=$dab_mode, sample_rate=$samp_rate, verbose=False), $address, $size, $protection, $verbose, $debug)</make>
  <param>
    <name>DAB Mode</name>
    <key>dab_mode</key>
    <value>1</value>
    <type>int</type>
    <option>
    	<name>Mode 1</name>
    	<key>1</key>
    </option>
    <option>
    	<name>Mode 2</name>
    	<key>2</key>
    </option>
    <option>
    	<name>Mode 3</name>
    	<key>3</key>
    </option>
    <option>
    	<name>Mode 4</name>
    	<key>4</key>
    </option>
  </param>
  <param>
    <name>Sampling Rate</name>
    <key>samp_rate</key>
    <value>samp_rate</value>
    <type>int</type>
  </param>
  <param>  
    <name>Subchannel address</name>
    <key>address</key>
    <type>int</type>
  </param>
  <param>
    <name>Subchannel size</name>
    <key>size</key>
    <type>int</type>
  </param>
  <param>
    <name>Protection mode</name>
    <key>protection</key>
    <type>int</type>
    <option>
    	<name>A1</name>
    	<key>0</key>
    </option>
    <option>
    	<name>A2</name>
    	<key>1</key>
    </option>
    <option>
    	<name>A3</name>
    	<key>2</key>
    </option>
    <option>
    	<name>A4</name>
    	<key>3</key>
    </option>
  </param>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <type>bool</type>
    <option>
    	<name>True</name>
    	<key>True</key>
    </option>
    <option>
    	<name>False</name>
    	<key>False</key>
    </option>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <type>bool</type>
    <option>
    	<name>True</name>
    	<key>True</key>
    </option>
    <option>
    	<name>False</name>
    	<key>False</key>
    </option>
  </param>
  <sink>
    <name>MSC symbols</name>
    <type>float</type>
    <vlen>1536*2</vlen>
  </sink>
  <sink>
    <name>trigger</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
