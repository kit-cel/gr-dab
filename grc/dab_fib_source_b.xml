<block>
  <name>DAB: FIB source</name>
  <key>dab_fib_source_b</key>
  <category>[DAB]</category>
  <import>import dab</import>
  <make>dab.fib_source_b($transmission_mode, $coutry_ID, $num_subch, $ensemble_label, $programme_service_labels, "", $service_comp_lang, $protection_mode, $data_rate_n, $dabplus)</make>
  <param>
    <name>Mode</name>
    <key>transmission_mode</key>
    <type>int</type>
    <option>
    	<name>Mode 1</name>
    	<key>1</key>
    </option>
    <option>
    	<name>Mode 2</name>
    	<key>2</key>
    </option>
    <option>
    	<name>Mode 3</name>
    	<key>3</key>
    </option>
    <option>
    	<name>Mode 4</name>
    	<key>4</key>
    </option>
  </param>
  <param>
    <name>Country ID</name>
    <key>coutry_ID</key>
    <type>int</type>
  </param>
  <param>
    <name>Number of sub-channels</name>
    <key>num_subch</key>
    <type>int</type>
  </param>
  <param>
    <name>Ensemble label</name>
    <key>ensemble_label</key>
    <type>string</type>
  </param>
  <param>
    <name>Programme service labels</name>
    <key>programme_service_labels</key>
    <type>string</type>
  </param>
  <param>
    <name>Service language</name>
    <key>service_comp_lang</key>
    <type>raw</type>
  </param>
  <param>
    <name>Protection modes</name>
    <key>protection_mode</key>
    <type>raw</type>
  </param>
  <param>
    <name>Data rates / 8kbit/s</name>
    <key>data_rate_n</key>
    <type>raw</type>
  </param>
  <param>
    <name>Dabplus</name>
    <key>dabplus</key>
    <type>raw</type>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
